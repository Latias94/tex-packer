{
  "atlas": [
  {{#each pages}}
    {
      "image": "{{this.image}}",
      "size": { "w": {{this.size.w}}, "h": {{this.size.h}} },
      "regions": [
      {{#each this.sprites}}
        {"name":"{{this.name}}","region":{"x":{{this.frame.x}},"y":{{this.frame.y}},"w":{{this.frame.w}},"h":{{this.frame.h}}},"rotated":{{this.rotated}},"source":{"x":{{this.sprite_source_size.x}},"y":{{this.sprite_source_size.y}},"w":{{this.sprite_source_size.w}},"h":{{this.sprite_source_size.h}}}}
        {{#unless @last}},{{/unless}}
      {{/each}}
      ]
    }
    {{#unless @last}},{{/unless}}
  {{/each}}
  ],
  "meta": {"app":"{{meta.app}}","version":"{{meta.version}}","format":"{{meta.format}}","scale":{{meta.scale}}}
}
